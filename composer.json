{
    "name": "deniztezcan/laravel-bolcom-retailer-api",
    "description": "Laravel package for the Bol.com v5 Retailer API",
    "type": "library",
    "license": "MIT",
    "version": "2.0.3",
    "homepage": "https://github.com/deniztezcan/laravel-bolcom-retailer-api",
    "authors": [
        {
            "name": "Deniz Tezcan",
            "email": "deniz@tezcanit.solutions"
        },
        {
            "name": "Jasper Verbeet",
            "email": "jasperverbeet@gmail.com"
        }
    ],
     "keywords": [
        "api",
        "php",
        "bol",
        "bolcom",
        "v5",
        "retailer",
        "laravel"
    ],
    "autoload": {
        "psr-4": { "DenizTezcan\\BolRetailer\\" : "src/" }
    },
    "minimum-stability": "stable",
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/support": "5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^7.0|^7.0",
        "nesbot/carbon": "^2.16"
    },
    "extra": {
      "laravel": {
        "providers": [
          "DenizTezcan\\BolRetailer\\BolServiceProvider"
        ],
        "aliases": {
          "BolRetailerAPI": "DenizTezcan\\BolRetailer\\Facades\\BolRetailerAPI"
        }
      }
    }
}
